msgid "None"
msgstr "Ingen"

#: ../src/screenshot-interactive-dialog.c:149
msgid "Drop shadow"
msgstr "Skygge"

#: ../src/screenshot-interactive-dialog.c:150
msgid "Border"
msgstr "Kant"

