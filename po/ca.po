msgid "None"
msgstr "Sense"

#: ../src/screenshot-interactive-dialog.c:155
msgid "Drop shadow"
msgstr "Ombra"

#: ../src/screenshot-interactive-dialog.c:156
msgid "Border"
msgstr "Vora"

