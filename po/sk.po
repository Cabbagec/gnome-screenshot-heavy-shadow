msgid "None"
msgstr "Žiadny"

#: ../src/screenshot-interactive-dialog.c:166
msgid "Drop shadow"
msgstr "Vrhnutý tieň"

#: ../src/screenshot-interactive-dialog.c:167
msgid "Border"
msgstr "Okraj"

